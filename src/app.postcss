/*place global styles here */
html, body { @apply h-full overflow-hidden; }

:root {
    --theme-rounded-container: theme(borderRadius.sm);
    --theme-rounded-base: theme(borderRadius.sm);

    /* =~= Theme Properties =~= */
    --theme-font-family-base: system-ui;
    --theme-font-family-heading: system-ui;
    --theme-font-color-base: 0 0 0;
    --theme-font-color-dark: 255 255 255;
    --theme-rounded-base: 0px;
    --theme-rounded-container: 8px;
    --theme-border-base: 1px;
    /* =~= Theme On-X Colors =~= */
    --on-primary: 0 0 0;
    --on-secondary: 0 0 0;
    --on-tertiary: 255 255 255;
    --on-success: 255 255 255;
    --on-warning: 0 0 0;
    --on-error: 0 0 0;
    --on-surface: 255 255 255;
    /* =~= Theme Colors  =~= */
    /* primary | #80f789 */
    --color-primary-50: 236 254 237; /* ⬅ #ecfeed */
    --color-primary-100: 230 253 231; /* ⬅ #e6fde7 */
    --color-primary-200: 223 253 226; /* ⬅ #dffde2 */
    --color-primary-300: 204 252 208; /* ⬅ #ccfcd0 */
    --color-primary-400: 166 249 172; /* ⬅ #a6f9ac */
    --color-primary-500: 128 247 137; /* ⬅ #80f789 */
    --color-primary-600: 115 222 123; /* ⬅ #73de7b */
    --color-primary-700: 96 185 103; /* ⬅ #60b967 */
    --color-primary-800: 77 148 82; /* ⬅ #4d9452 */
    --color-primary-900: 63 121 67; /* ⬅ #3f7943 */
    /* secondary | #dde06d */
