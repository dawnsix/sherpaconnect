    --color-secondary-100: 248 249 226; /* ⬅ #f8f9e2 */
    --color-secondary-200: 247 247 219; /* ⬅ #f7f7db */
    --color-secondary-300: 241 243 197; /* ⬅ #f1f3c5 */
    --color-secondary-400: 231 233 153; /* ⬅ #e7e999 */
